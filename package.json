{
  "name": "express-typescript",
  "version": "1.0.0",
  "description": "Example of express with typescript",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:live",
    "build": "tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "express-typescript"
  },
  "keywords": [
    "express",
    "typescript"
  ],
  "author": "dkozak",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@types/node": "^11.10.4",
    "nodemon": "^1.18.10",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3333"
  }
}
